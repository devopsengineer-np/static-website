pipeline {
    agent any
    stages {
        stage('git checkout') {
            steps {
                checkout([$class: 'GitSCM',
                          branches: [[name: 'feature/sumesh']],
                          userRemoteConfigs: [[credentialsId: 'bcc02395-b9e0-490f-abb1-b00bdf3cad0d',
                                               url: 'https://github.com/devopsengineer-np/static-website.git']]])
            }
        }
        stage('list all') {
            steps {
                sh 'ls -al'
            }
        }
        stage('display') {
            steps {
                sh 'cat index.html'
            }
        }
    }
}
